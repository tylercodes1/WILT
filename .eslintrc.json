{
   "env": {
      "browser": true,
      "es2021": true
   },
   "extends": [
      "next",
      "next/core-web-vitals",
      "eslint:recommended",
      "prettier/react",
      "airbnb",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
   ],
   "parserOptions": {
      "ecmaFeatures": {
         "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
   },
   "plugins": ["react", "prettier"],
   "rules": {
      "prettier/prettier": "error"
   }
}
